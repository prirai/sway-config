{
  "layer": "top",
  "position": "top",
  "spacing": 0,
  "height": 32,
  "margin": 10,
  "gtk-layer-shell": true,
  "ipc": true,

  "modules-left": ["custom/launcher", "sway/workspaces", "custom/current-mode"],

  "modules-center": ["idle_inhibitor", "sway/window"],

  "modules-right": [
    "pulseaudio",
    "network",
    "bluetooth",
    "cpu",
    "memory",
    "battery",
    "tray",
    "custom/power",
    "clock"
  ],

"idle_inhibitor": {
  "format": "{icon}",
  "format-icons": {
    "activated": "\uF023",
    "deactivated": "\uF3C1"
  },
  "tooltip": true,
  "tooltip-format-activated": "Idle inhibitor is active (prevents screen lock)",
  "tooltip-format-deactivated": "Idle inhibitor is off (permits screen lock)"
},

  "sway/window": {
    "format": "{title}",
    "max-length": 50,
    "tooltip": true,
    "tooltip-format": "{app_id}",
    "on-click-middle": "swaymsg kill"
  },

  "custom/launcher": {
    "format": "\uF568",
    "on-click": "rofi -show drun",
    "tooltip": false
  },

  "sway/workspaces": {
    "format": "{name}",
    "all-outputs": true,
    "persistent_workspaces": {
      "1": [],
      "2": [],
      "3": [],
      "4": [],
      "5": []
    }
  },

  "custom/current-mode": {
    "exec": "swaymsg -t get_mode | jq -r .mode",
    "interval": 1,
    "format": "{}",
    "tooltip": false
  },

  "clock": {
    "format": "{:%H:%M}",
    "tooltip-format": "{:%A, %d %B %Y}",
    "interval": 60,
    "on-click": "wldash"
  },

  "cpu": {
    "format": "ï‹› {usage}%",
    "interval": 2,
    "states": {
      "critical": 90
    }
  },

  "memory": {
    "format": "ï”¸ {percentage}%",
    "interval": 2,
    "states": {
      "critical": 80
    }
  },

  "battery": {
    "format": "{icon}",
    "format-icons": ["ï‰„", "ï‰ƒ", "ï‰‚", "ï‰", "ï‰€"],
    "states": {
      "warning": 30,
      "critical": 15
    },
    "interval": 5,
    "tooltip-format": "{capacity}%"
  },

  "network": {
    "format-wifi": "ï‡«",
    "format-ethernet": "ó°ˆ {bandwidthDownBits}",
    "format-disconnected": "ó°–ª",
    "interval": 5,
    "tooltip-format": "ğŸ“¡ {ifname} | {essid} ({signalStrength}%)\nâ¬‡ {bandwidthDownBits} â¬† {bandwidthUpBits}",
    "on-click": "alacritty -e nmtui"
  },

  "bluetooth": {
    "format": "ïŠ”",
    "format-connected": "ïŠ” {device_alias}",
    "format-disabled": "ïŠ”",
    "interval": 5,
    "on-click": "bluejay"
  },

  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-bluetooth": "ïŠ” {volume}%",
    "format-muted": "ó°– Muted",
    "format-icons": {
      "headphone": "ó°‹‹",
      "hands-free": "ó°‹",
      "headset": "ó°‹",
      "phone": "ï‚•",
      "portable": "ï‚•",
      "car": "ï†¹",
      "default": ["ó°•¿", "ó°–€", "ó°•¾"]
    },
    "on-click": "pavucontrol-qt",
    "scroll-step": 2
  },

  "custom/power": {
    "format": "â»",
    "on-click": "wlogout --protocol layer-shell",
    "tooltip": false
  }
}
